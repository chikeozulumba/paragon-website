{
  "version": 3,
  "routes": [
    {
      "src": "^/_astro/(.*)$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "handle": "error"
    },
    {
      "status": 404,
      "src": "^(?!/api).*$",
      "dest": "/404.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "render"
    }
  ],
  "framework": {
    "version": "3.2.4"
  },
  "crons": [],
  "flags": []
}
