---
import Layout from "../layouts/Layout.astro";
import Booking from "../components/pages/advisory/Booking.astro";
import Footer from "../components/pages/Footer.astro";
import AlternateHero from "../components/hero/Secondary.astro";
import Founder from "../components/pages/Founder.astro";
import Testimonial from "../components/pages/Testimonial.astro";
import Newsletter from "../components/pages/Newsletter.astro";
---

<Layout title="Express Advisory | Paragon Consulting" page="advisory-sessions">
  <AlternateHero 
    captions={{
      top: "Express Advisory",
      description: "Need quick advice and want to talk out a challenge or an idea?",
    }} />
  <Booking />
  <Founder class="md:pb-[144px] md:pt-[200px] pt-[50px]" />
  <Testimonial />
  <Newsletter />
  <Footer />
</Layout>
